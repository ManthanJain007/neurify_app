{
  "version": 2,
  "builds": [
    {
      "src": "ai-engine/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "neurowrite-app/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "ai-engine/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "neurowrite-app/$1"
    }
  ]
}
